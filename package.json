{
  "name": "project_manager",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build ",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint src",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint -e",
    "prettier": "prettier --write .",
    "prettier:staged": "pretty-quick --staged --verbose",
    "prepare": "node ./.husky/skip.js || husky install",
    "hooks:pre-commit": "run-s prettier:staged lint:staged",
    "hooks:pre-push": "run-s lint:commit"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@prettier/plugin-xml": "^3.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prettier-plugin-properties": "^0.3.0",
    "prettier-plugin-svelte": "^3.1.0",
    "pretty-quick": "^3.1.3",
    "svelte": "^4.2.5",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@ui5/webcomponents": "^1.19.1",
    "@ui5/webcomponents-base": "^1.19.1",
    "@ui5/webcomponents-fiori": "^1.19.1",
    "@ui5/webcomponents-icons": "^1.19.1",
    "@ui5/webcomponents-icons-business-suite": "^1.19.1",
    "@ui5/webcomponents-icons-tnt": "^1.19.1"
  }
}
